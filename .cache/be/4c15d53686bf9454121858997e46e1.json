state;\r\n    return (\r\n      <div className=\"details\">\r\n        <Carousel media={media} />\r\n        <div>\r\n          <h1>{name}</h1>\r\n          <h2>\r\n            {animal}-{bread}-{location}\r\n          </h2>\r\n          <button onClick={this.toggleModal}>Adopt {name}</button>\r\n          <p>{description}</p>\r\n          {showModal ? (\r\n            <Modal>\r\n              <h1>Would you like to adopt {name}!</h1>\r\n              <button className=\"modal-button\" onClick={this.toggleModal}>\r\n                Yes\r\n              </button>\r\n              <button className=\"modal-button\" onClick={this.toggleModal}>\r\n                No\r\n              </button>\r\n            </Modal>\r\n          ) : null}\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n\r\nexport default Details;\r\n"},"lineCount":null}},"error":null,"hash":"d33998e680de95ae1459f9726732b052","cacheData":{"env":{"API_KEY":"HWCNVPAsGns84YlUagMBgf22aRg77D1cNl6Bf2KPKiWv6NI7OB","API_SECRET":"AlOsDdJC7zPvNTiNiLL4hOXBC6oDji8unAEKuYLP"}}}